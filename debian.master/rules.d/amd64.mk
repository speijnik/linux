human_arch	= 64 bit x86
build_arch	= x86_64
header_arch	= $(build_arch)
asm_link	= x86
defconfig	= defconfig
flavours	= generic virtual
build_image	= bzImage
kernel_file	= arch/$(build_arch)/boot/bzImage
